package backEnd.exceptions;


/**
 * @author Patricio Reller, German Romarion, Gabriel Zanzotti
 * 
 *         This class represents a RuntimeException that can occur when some
 *         parameters in the '.board' file are wrong while parsed.
 */
public class ReadErrorException extends Exception {
	private static final long serialVersionUID = 1L;

	public ReadErrorException() {
		super();
	}
	
	public ReadErrorException(String message) {
		super(message);
	}
}
