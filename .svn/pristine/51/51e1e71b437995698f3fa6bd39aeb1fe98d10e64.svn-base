package backEnd.exceptions;


/**
 * @author Patricio Reller, German Romarion, Gabriel Zanzotti
 * 
 *         This class represents a RuntimeException that can occur for example
 *         when the wall is asked to reacto against a Hero object, which
 *         technically can't happen.
 */
public class GameLogicException extends RuntimeException {

	private static final long serialVersionUID = 1L;

	public GameLogicException(String message) {
		super(message);
	}
}
